#include <stdio.h>
#include <stdlib.h>

void my_strcpy(char*dest,char*str)
{
    while(*str!='\0')
    {
          *dest=*str;
         str++;
          dest++;
    }
        *dest=*str;
}

 int main()
 {
     char arr1[]="#########";
     char arr2[]="bit";
     my_strcpy(arr1,arr2);
     printf("%s",arr2);
   return 0;
}
